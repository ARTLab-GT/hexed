PROJECT_NAME = hexed
OUTPUT_DIRECTORY = doc
INPUT = @CMAKE_SOURCE_DIR@/include @CMAKE_SOURCE_DIR@/src @CMAKE_SOURCE_DIR@/doc @CMAKE_SOURCE_DIR@/script @CMAKE_SOURCE_DIR@/hexed_config.hpp.in
GENERATE_TREEVIEW = YES
DISABLE_INDEX = YES
USE_MATHJAX = YES
EXTENSION_MAPPING += .in=C++
FILE_PATTERNS += *.in
WARN_IF_UNDOCUMENTED = NO
GENERATE_LATEX = NO
PREDEFINED = HEXED_USE_NLOPT=1 HEXED_USE_OTTER=1 HEXED_USE_TECPLOT=1 HEXED_THREADED=1
VERBATIM_HEADERS = NO
