PROJECT_NAME = hexed
PROJECT_NUMBER = @CMAKE_PROJECT_VERSION@
INPUT = @CMAKE_SOURCE_DIR@/include @CMAKE_SOURCE_DIR@/src @CMAKE_SOURCE_DIR@/python @CMAKE_SOURCE_DIR@/doc @CMAKE_SOURCE_DIR@/script @CMAKE_SOURCE_DIR@/hexed_config.hpp.in
GENERATE_TREEVIEW = YES
DISABLE_INDEX = YES
USE_MATHJAX = YES
FILE_PATTERNS += *.in *.py
EXTENSION_MAPPING += in=c++
WARN_IF_UNDOCUMENTED = NO
GENERATE_LATEX = NO
GENERATE_TAGFILE = hexed.tag
PREDEFINED = HEXED_USE_NLOPT=1 HEXED_USE_OTTER=1 HEXED_USE_TECPLOT=1 HEXED_USE_XDMF=1 HEXED_THREADED=1 HEXED_USE_OCCT=1
VERBATIM_HEADERS = NO
TAGFILES = @CMAKE_SOURCE_DIR@/doc/occt_short.tag=https://dev.opencascade.org/doc/refman/html \
           @CMAKE_SOURCE_DIR@/doc/Eigen.doxytags=https://eigen.tuxfamily.org/dox
EXAMPLE_PATH = @CMAKE_SOURCE_DIR@
HTML_EXTRA_STYLESHEET = @CMAKE_SOURCE_DIR@/doc/style.css
ALIASES += today="@DATE@" proj_version=" version @CMAKE_PROJECT_VERSION@, commit `@COMMIT@`" me="\ref Contact \"me\""
IMAGE_PATH = html
